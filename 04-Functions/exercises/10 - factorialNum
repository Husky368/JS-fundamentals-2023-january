function factorialNum(first, second) {
    let sum = 0
    let last = 0
    for (i = first - 1; i >= 2; i--) {
        let current = 0
        if (i == first-1) {
            current = i * first
            last = current
        }
        else {
            current = last * i
            last = current
        }
        sum = current
    }
    let result = sum / second
    console.log(result.toFixed(2))
}
